<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.controller.AdminController"
            prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="lb_nombre" style="-fx-font-size: 18;"/>
    <TableView fx:id="tbl_users" prefHeight="400" prefWidth="500" />
    <HBox spacing="10">
        <Button fx:id="btn_add_user" text="Agregar" onAction="#agregarUsuario"/>
        <Button fx:id="btn_del_user" text="Eliminar" onAction="#eliminarUsuario"/>
    </HBox>

</AnchorPane>
